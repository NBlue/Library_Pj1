<div class="container-users">
    <div class="book">
        <div class="grid wide">
            <div class="book__list">
                <div class="row">
                    {{#each data}}
                    <div class="col l-2-4">
                        <div class="book__item" data-id="{{this.IdBook}}">
                            <section class="book__item-display">
                                <section class="book__item-display-img" style="background-image: url(/img/books/{{this.ImgBook}});"></section>
                            </section>
                            <div class="book__item-hover">
                                <p class="book__item-hover-name">{{this.Name}}</p>
                                <p class="book__item-hover-author">{{this.NameAuthor}}</p>
                                <section class="book__item-hover-like">
                                    <p>Lượt thích: {{this.LikeBook}} </p>
                                    <i class="fas fa-heart"></i>
                                </section>
                                <a class="btn btn__detail">Xem chi tiết</a>
                                <button class="btn btn__add-cart">Thêm vào giỏ</button>
                            </div>
                        </div>
                    </div>
                    {{/each}}
                </div>
            </div>
            <div class="pagination">
                <ul class="pagination-list">
                    <li class="pagination-item">
                        <a href="" class="pagination-prev"><i class="fas fa-angle-double-left"></i></a>
                    </li>
                    <li class="pagination-item">
                        <a href="" class="pagination-number">1</a>
                    </li>
                    <li class="pagination-item">
                        <a href="" class="pagination-number">2</i></a>
                    </li>
                    <li class="pagination-item">
                        <a href="" class="pagination-number">3</i></a>
                    </li>
                    <li class="pagination-item">
                        <a href="" class="pagination-next"><i class="fas fa-angle-double-right"></i></a>
                    </li>
                </ul>
            </div>
        </div>
    </div>
</div>                        